snippet rt
	<rt>${1}</rt>
snippet rp
	<rp>${1}</rp>
snippet section
	<section>
		${3}
	</section>
snippet script Embebido
	<script type="text/javascript" charset="utf-8">
		${1}
	</script>
	${2}
snippet script Archivo
	<script src="${1}.js" type="text/javascript" charset="utf-8"></script>
	${2}
snippet style
	<style type="text/css" media="${1:screen}">
		${1:codigo}
	</style>
snippet select
		<select name="${1:some_name}">
		<option value="${3:option}">${4:$3}</option>
	</select>${5}
snippet source
	<source src="${1:src}" type="${2:type}">
snippet summary
	<summary>${1}</summary>
snippet time
	<time datetime="${1}">${2}</time>
snippet td
	<td>${1}</td>
snippet tr
	<tr>${1}</tr>
snippet title
	<title>${1:Titulo}</title>
	${2}
snippet table
	<table>
		<tr><th>${2:Header}</th></tr>
		<tr><th>${3:Data}</th></tr>
	</table>${4}
snippet textarea
	<textarea name="${3:nombre}" rows="${4:no.filas}" cols="${5:no.columnas}">${6}</textarea>${7}
snippet video
	<video width="${1}" height="${2}" controls>
		${3}
	</video>
snippet xhtml
	<html xmlns="http://www.w3.org/1999/xhtml">
		${1}
	</html>

